<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codingstuff.loginandsignup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_Jalpa_launcher"
        android:label="Reservaciones Jalpa De Canovas"
        android:roundIcon="@mipmap/ic_Jalpa_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginAndSignUp">
        <activity
            android:name=".MainActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".activity"
            android:exported="false"
            android:label="@string/title_activity_2"
            android:theme="@style/Theme.LoginAndSignUp.NoActionBar" />
        <activity
            android:name=".SignInActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>